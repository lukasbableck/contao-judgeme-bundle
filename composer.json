{
	"name": "lukasbableck/contao-judgeme-bundle",
	"type": "contao-bundle",
	"description": "Integrate Judge.me reviews into your Contao website.",
	"keywords": [
		"contao",
		"judgeme",
		"reviews",
		"shopify"
	],
	"require": {
		"contao/core-bundle": "^4.13 || ^5.0"
	},
	"license": "LGPL-3.0-or-later",
	"autoload": {
		"psr-4": {
			"Lukasbableck\\ContaoJudgemeBundle\\": "src/"
		}
	},
	"authors": [
		{
			"name": "Lukas Bableck",
			"email": "lukas@bableck.dev"
		}
	],
	"conflict": {
		"contao/manager-plugin": "<2.0 || >=3.0"
	},
	"require-dev": {
		"contao/manager-plugin": "^2.0",
		"friendsofphp/php-cs-fixer": "^3.57"
	},
	"extra": {
		"contao-manager-plugin": "Lukasbableck\\ContaoJudgemeBundle\\ContaoManager\\Plugin"
	}
}